{
  "build": {
    "env": {
      "HUGO_VERSION": "0.25.1"
    }
  },
  "github": {
    "silent": true
  },
  "trailingSlash": true,
  "redirects": [

    {
      "source": "/cv/",
      "destination": "/en/vitae/"
    },

    {
      "statusCode": 301,
      "source": "/gh/(.*)",
      "destination": "https://github.com/xiangyazi24/$1"
    },
    {
      "source": "/([^/]+)/guestbook.php",
      "destination": "/$1/about/"
    }
  ]
}
